{
    "name": "ttarnawski/ickle",
    "description": "ickle is a url shortener to reduce a long link.",
    "authors": [
        {
            "name": "Tomasz Tarnawski",
            "email": "tarnawski27@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-pdo": "*",
        "ext-json": "*",
        "ramsey/uuid": "^4.1",
        "pimple/pimple": "^3.3",
        "symfony/messenger": "^5.1",
        "symfony/config": "^5.1",
        "symfony/http-kernel": "^5.1",
        "symfony/http-foundation": "^5.1",
        "symfony/routing": "^5.1",
        "symfony/dependency-injection": "^5.1",
        "symfony/framework-bundle": "^5.1",
        "symfony/validator": "^5.1",
        "monolog/monolog": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "squizlabs/php_codesniffer": "^3.5",
        "phpstan/phpstan": "^0.12.54",
        "phpmd/phpmd": "^2.9"
    },
    "archive": {
        "exclude": [
            "docker",
            "tests",
            "var",
            "docker-compose.yml",
            "phpunit.xml.dist",
            "Makefile",
            "README.md",
            "Jenkinsfile",
            ".git",
            ".gitignore"
        ]
    }
}
